{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "static",
    "rootDir": "./src",
    "resolveJsonModule": true,
    "lib": ["DOM", "ESNext"]
  },
  "include": ["src/**/*"]
}
