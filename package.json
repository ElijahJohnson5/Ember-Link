{
  "name": "@ember-link/workspace",
  "private": true,
  "version": "0.0.1",
  "packageManager": "yarn@4.6.0",
  "author": "Elijah Johnson <elijahjohnson987@gmail.com>",
  "license": "MIT",
  "scripts": {
    "publish:packages": "SCRIPT=\"$(cat ./scripts/publish-if-needed.sh)\" yarn workspaces foreach -Ap --topological-dev --no-private --exclude '@ember-link/workspace' exec 'echo \"$SCRIPT\" | bash'",
    "version": "yarn changeset version && YARN_ENABLE_IMMUTABLE_INSTALLS=false yarn install",
    "build": "moon run :build --query 'projectSource~packages/*'"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "examples/*"
  ],
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@eslint/js": "^9.17.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.14.0",
    "prettier": "^3.1.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.28.0"
  },
  "engines": {
    "node": "22.12.0"
  }
}
